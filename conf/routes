# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET         /                                 controllers.InspectionController.index

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file                     controllers.Assets.versioned(path="/public", file: Asset)

GET         /inspections                      controllers.InspectionController.getInspections
GET         /inspections/:inspectionID        controllers.InspectionController.getInspection(inspectionID : Long)
POST        /inspections                      controllers.InspectionController.createInspectionConfiguration
PUT         /inspections/:inspectionID        controllers.InspectionController.updateInspectionConfiguration(inspectionID : Long)